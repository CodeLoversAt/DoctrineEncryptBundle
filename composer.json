{
    "name": "codelovers/doctrine-encrypt-bundle",
    "description": "Symfony 2 Bundle for storing encrypted data in a Symfony 2 project, using doctrine ORM or ODM",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Holzmann",
            "email": "d@velopment.at"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-mcrypt": "*",
        "symfony/framework-bundle": ">=2.1"
    },
    "require-dev": {
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/mongodb-odm": "1.0.*@dev",
        "doctrine/mongodb-odm-bundle": "3.0.*@dev"
    },
    "suggest": {
        "doctrine/orm": "For usage with doctrine ORM",
        "doctrine/doctrine-bundle": "For usage with doctrine ORM",
        "doctrine/mongodb-odm": "For usage with doctrine mongodb ODM",
        "doctrine/mongodb-odm-bundle": "For usage with doctrine mongodb ODM"
    },
    "autoload": {
        "psr-0": {
            "CodeLovers\\DoctrineEncryptBundle": ""
        }
    },
    "target-dir": "CodeLovers/DoctrineEncryptBundle"
}
